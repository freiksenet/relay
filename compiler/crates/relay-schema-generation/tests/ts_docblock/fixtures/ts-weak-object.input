//- module.ts

import type CatFlowType from 'Cat';

/**
 * @RelayResolver
 */
export function Cat(id: DataID): CatFlowType {
  return {};
}

/**
 * @RelayResolver
 */
export type FullName = {first: string, last: string};


/**
 * @RelayResolver
 */
export function first(name: FullName): string | null | undefined {
  return name.first;
}

/**
 * @RelayResolver
 */
export function last(name: FullName):  string | null | undefined {
  return name.last;
}

/**
 * @RelayResolver
 */
export function fullName(cat: CatFlowType): FullName | null | undefined {
  return {first: "Mr", last: "Cat"};
}
